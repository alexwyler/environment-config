#
# Config Stuff -- source .customrc
#

export EDITOR="subl -w"
export GIT_EDITOR="subl -w"
export PATH="$PATH:~/arcanist/bin:~/bin"
alias ab="arc build"
alias gs="git status"
alias gr="git fetch; git rebase trunk"
alias gc="git commit -a"
alias gco="git checkout"
alias ga="gc --amend -C HEAD"
alias ad="ga; arc diff"
alias au="ga; arc unit"
alias gb="git checkout HEAD^"
alias gp="git pull --rebase && git push"
alias e="subl -w"
alias gediff="git difftool --tool=ediffclient"
